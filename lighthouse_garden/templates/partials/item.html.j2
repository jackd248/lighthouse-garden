<tr id="{{identifier}}">
    <td>
        <strong>{{title}}</strong><br/>
        <a href="{{url}}" target="_blank">{{url}}</a>
    </td>
    <td>
        <div class="dropdown">
                <div class="dropdown-toggle" id="dropdown-{{identifier}}" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        &#8230;
                </div>
                <div class="dropdown-menu" aria-labelledby="dropdown-{{identifier}}">
                        <a class="dropdown-item share-link" href="#{{identifier}}">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-share" viewBox="0 0 16 16">
                                  <path d="M13.5 1a1.5 1.5 0 1 0 0 3 1.5 1.5 0 0 0 0-3zM11 2.5a2.5 2.5 0 1 1 .603 1.628l-6.718 3.12a2.499 2.499 0 0 1 0 1.504l6.718 3.12a2.5 2.5 0 1 1-.488.876l-6.718-3.12a2.5 2.5 0 1 1 0-3.256l6.718-3.12A2.5 2.5 0 0 1 11 2.5zm-8.5 4a1.5 1.5 0 1 0 0 3 1.5 1.5 0 0 0 0-3zm11 5.5a1.5 1.5 0 1 0 0 3 1.5 1.5 0 0 0 0-3z"/>
                                </svg>
                                Link to share
                        </a>
                        <a class="dropdown-item" href="{{last_report}}">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-earmark-bar-graph" viewBox="0 0 16 16">
                                  <path d="M10 13.5a.5.5 0 0 0 .5.5h1a.5.5 0 0 0 .5-.5v-6a.5.5 0 0 0-.5-.5h-1a.5.5 0 0 0-.5.5v6zm-2.5.5a.5.5 0 0 1-.5-.5v-4a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5v4a.5.5 0 0 1-.5.5h-1zm-3 0a.5.5 0 0 1-.5-.5v-2a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5v2a.5.5 0 0 1-.5.5h-1z"/>
                                  <path d="M14 14V4.5L9.5 0H4a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2zM9.5 3A1.5 1.5 0 0 0 11 4.5h2V14a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h5.5v2z"/>
                                </svg>
                                Last report
                        </a>
                </div>
        </div>
    </td>
    <td>
        {{circle_average}}
    </td>
    <td class="pb-0">
        <div id="graph-{{identifier}}" class="graph" data-values-y="{{graph_values_y}}" data-values-x="{{graph_values_x}}" data-values-text="{{graph_values_text}}"></div>
    </td>
    <td>
        {{circle_performance}}
    </td>
    <td>
        {{circle_accessibility}}
    </td>
    <td>
        {{circle_best_practices}}
    </td>
    <td>
        {{circle_seo}}
    </td>
</tr>